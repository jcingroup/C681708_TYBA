@using OutWeb.Models.Manage.ResultModels
@model ResultDetailsDataModel

@{
    ViewBag.Title = Model.Data.TITLE;
    ViewBag.BodyClass = "Result";
    ViewBag.ContentClass ="text-left ind pb-24";
}

<h4 class="title text-left ml-72">
    <span class="underline">@Model.Data.TITLE</span>
    <small class="block font-sm text-success mt-12">@Model.Data.PUB_DT_STR</small>
</h4>

<article class="editor ml-72">
    @* 活動內容(編輯器) *@
    @Html.Raw(Model.Data.CONTENT)
</article>

<hr class="mb-12">

<a href="~/Result" class="back pull-right ti-arrow-left">回列表</a>