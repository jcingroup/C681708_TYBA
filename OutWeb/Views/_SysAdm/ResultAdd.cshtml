@using OutWeb.Models.Manage.ResultModels
@model ResultDetailsDataModel

@{
    Layout = "~/Views/Shared/_MLayout.cshtml";
    ViewBag.Crumb = "比賽成績公告";
    ViewBag.subnav = "General";
    ViewBag.subnav2 = "Result";
}

@section Breadcrumb {
    <ul class="breadcrumb">
        <li>一般頁面管理</li>
        <li>@ViewBag.Crumb</li>
    </ul>
}

<h3 class="title">@ViewBag.Crumb <small class="oi" data-glyph="tags">新增</small></h3>

@using (Html.BeginForm("ResultSave", "_SysAdm", FormMethod.Post, new { id = "frmResultData", @class = "form-list" }))
{
    Html.RenderPartial("_ResultDataPartial", Model);
}