@using OutWeb.Models.Manage.ManageNewsModels
@model NewsDetailsDataModel
@{
    Layout = "~/Views/Shared/_MLayout.cshtml";
    ViewBag.Crumb = "比賽訊息管理";
    ViewBag.subnav = "General";
    ViewBag.subnav2 = "News";
}

@section Breadcrumb {
    <ul class="breadcrumb">
        <li>一般頁面管理</li>
        <li>@ViewBag.Crumb</li>
    </ul>
}

<h3 class="title">@ViewBag.Crumb <small class="oi" data-glyph="tags">編輯</small></h3>

@using (Html.BeginForm("NewsSave", "_SysAdm", FormMethod.Post, new { id = "formManageNewsData", @class = "form-list" }))
{
    Html.RenderPartial("_NewsDataPartial", Model);
}